<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent" android:layout_height="wrap_content"
>
    <TextView android:id="@+id/item_tel"
              android:layout_width="wrap_content" android:layout_height="wrap_content"
              app:layout_constraintTop_toTopOf="parent"
              app:layout_constraintStart_toStartOf="parent" />
    <TextView android:id="@+id/pre_Chip_of_Permission"
              android:text="@string/chip_allowed"
              style="@style/Text.Little"
              android:layout_marginTop="@dimen/MarginReallyLittle"
              android:layout_width="wrap_content" android:layout_height="wrap_content"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toBottomOf="@id/item_tel" />
    <TextView android:id="@+id/item_chip"
              style="@style/Text.Little"
              android:layout_width="wrap_content" android:layout_height="wrap_content"
              android:layout_marginStart="@dimen/MarginReallyLittle"
              app:layout_constraintStart_toEndOf="@id/pre_Chip_of_Permission"
              app:layout_constraintTop_toTopOf="@id/pre_Chip_of_Permission" />
</android.support.constraint.ConstraintLayout>